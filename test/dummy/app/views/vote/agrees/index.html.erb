<h1>Listing vote_agrees</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @vote_agrees.each do |vote_agree| %>
      <tr>
        <td><%= vote_agree.user %></td>
        <td><%= link_to 'Show', vote_agree %></td>
        <td><%= link_to 'Edit', edit_vote_agree_path(vote_agree) %></td>
        <td><%= link_to 'Destroy', vote_agree, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Agree', new_vote_agree_path %>
